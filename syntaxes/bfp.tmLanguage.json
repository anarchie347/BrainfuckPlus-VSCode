{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "BrainfuckPlus",
	"patterns": [
		{
			"include": "#keywords"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.bfp",
				"match": "\\[|\\]"
			},
			{
				"name": "keyword.operator",
				"match": "+|-"
			},
			{
				"name": "keyword.move",
				"match": "<|>"
			}
			{
				"name": "keyword.io",
				"match": ".|,"

			}
		]
		}
	},
	"scopeName": "source.bfp"
}