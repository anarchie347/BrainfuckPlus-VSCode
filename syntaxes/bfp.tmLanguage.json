{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "BrainfuckPlus",
	"patterns": [
		{
			"include": "#keywords"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
			{
				"name": "keyword.operator.bfp",
				"match": "[\\+-]"
			},
			{
				"name": "keyword.move.bfp",
				"match": "[<>]"
			},
			{
				"name": "keyword.io.bfp",
				"match": "[\\.,]"
			},
			{
				"name": "keyword.debug.bfp",
				"match": "[\\\\:?\"\\|]"
			}
		]
		}
	},
	"scopeName": "source.bfp"
}